
<h1>Transactions</h1>
<%= Transaction.total_spent() %>
<table>
  <tr>
    <th>How Much?</th>
    <th></th>
    <th>Tag</th>
    <th></th>
    <th>Merchant</th>
    <th></th>
  </tr>
  <% for transaction in @transactions %>
  <tr>
    <td><%= transaction.value %></a></td>
    <td>|</td>
    <td><%= transaction.tag_id %></td>
    <td>|</td>
    <td><%= transaction.merchant.name %></td>
  </tr>
  <% end %>

</table>
  <p>Transaction by tag: <%= Transaction.total_spent_by_tag(@tag) %> </p>
